<h1>Coding Events</h1>

<p>
    <a asp-controller="Events" asp-action="Add">Add Event</a>
</p>

@if(ViewBag.events.Count == 0)
{
    <p>No events yet!</p>
}

 
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Event Name</th>
            <th scope="col">Event Type</th>
        </tr>
    </thead>
    <tbody>
        @foreach (KeyValuePair<string, string> events in ViewBag.Events)
        {
            <tr>
                <td>@events.Key</td>
                <td>@events.Value</td>
            </tr>
        }
    </tbody>
</table>